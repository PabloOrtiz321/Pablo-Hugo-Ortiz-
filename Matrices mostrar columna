#include <iostream>

using namespace std;


int main()
{
    int numero[10][10], filas, columnas; 
    cout<<"Ingrese el numero de filas "<<endl;
    cin>>filas;
    cout<<"Ingrese el numero de columnas"<<endl;
    cin>>columnas;

    for(int i=0; i<filas;i++){
        for(int j=0; j<columnas; j++){
            cout<<"Digite un numero "<<i<<""<<j<<endl;
            cin>>numero[i][j];
        }
    }
     for(int i=0; i<filas;i++){
        for(int j=0; j<columnas; j++){
            cout<<numero[j][i];
        }
     }

    return 0;
}
